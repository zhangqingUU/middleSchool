<mapper namespace="com.school.result.mapper.GradeMapper">
    <select id="selGraListByGId" resultType="com.school.result.pojo.Grade" parameterType="int">
        select gradeid,gradename,gradationId from grade where gradationId=#{gradationId}
    </select>
</mapper>